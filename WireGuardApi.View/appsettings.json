{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LogFilePath": "/Logs/log-{Date}.txt",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5003"
      }
    }
  },
  "WireGuard": {
    "WGConfigPath": "/etc/wireguard/",
    "WGClientsPath": "/etc/wireguard/clients"
  },
  "Tokens": "1"
}
